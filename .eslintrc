{
  "globals": {
    "qp": true,
    "define": true,
    "log": true
  },
  "rules": {
    "indent": 0,
    "quotes": [ 2, "single" ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 2, "always" ],
    "no-unused-vars": [ 0, { "vars": "all", "args": "none" } ]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}
